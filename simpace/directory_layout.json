{
	"dir": {
		"base": "./",
		"sub+": "sub{:02d}",
		"sess+": "sess{:02d}",
		"run+": "run{:02d}",
		"runs": "preproc",
		"signals": "extracted_signals",
		"realign": "realign",
		"smooth": "smooth"
	},
	"pat": {
		"sub+": "*sub{:02d}_",
		"sess+": "*sess{:02d}_",
		"run+": "*run{:02d}-",
		"sub+sess+": "*sub{:02d}_sess{:02d}_",
		"sess+run+": "*sess{:02d}_run{:02d}-",
		"sub+sess+run+": "*sub{:02d}_sess{:02d}_run{:02d}-"
	},
	"spm_mvt": {
		"mvtpref": "rp_",
		"mvtpost": "00?.txt",
		"mvtprm": "rp_asub??_sess??_run*-00*.txt",
		"mvtrun1+": "rp_asub{:02d}_sess{:02d}_run01-0*.txt"
	},
	"csf": {
		"@dir": "@dir@runs@csf",
		"dir": "csf_mask",
		"roi_mask": "csf_map_final.nii*"
	},
	"wm": {
		"@dir": "@dir@runs@wm",
		"dir": "wm_mask",
		"roi_mask": "wm_map_final.nii*"
	},
	"atlas": {
		"dir": "registered_files",
		"prepat": "rraal_*.nii"
	},
	"out": {
		"signals": {
			"dir": "extracted_signals",
			"signals+": "signal_sess_{:02d}_run{:02d}_",
			"f_signals+": "filtered_signal_sess_{:02d}_run{:02d}_"
		},
		"sess_mask": {
			"dir": "sess_mask",
			"roi_mask": "sess_mask.nii"
		}
	}
}

